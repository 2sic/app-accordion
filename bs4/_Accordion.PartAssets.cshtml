@inherits Custom.Hybrid.Razor12
@using ToSic.Razor.Blade;
@{
  // Activate font-Awesome 5 from the global resources and activate https://r.2sxc.org/turnon
  var page = GetService<ToSic.Sxc.Web.IPageService>();
  page.Activate("FontAwesome5", "turnOn");
}

<turnOn turn-on='{ "run": "window.appAccordion4.init()", "data": { "domId": "@DynamicModel.DomId" } }'></turnOn>
<script src="@App.Path/bs4/dist/scripts.min.js" data-enableoptimizations="true"></script>
<link rel="stylesheet" href="@App.Path/bs4/dist/styles.min.css" data-enableoptimizations="true">
